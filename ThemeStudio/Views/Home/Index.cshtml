@using ThemeStudio.Helper
@using ThemeStudio.Extensions
@using ThemeStudio.Helper.ScssHelper

<style id="custom-theme"></style>
<div class="theme-body-left-ovelay e-view" id="left-overlay">
    <div class="sb-loading">
        <svg class="circular" height="40" width="40">
            <circle class="path" cx="25" cy="25" r="20" fill="none" stroke-width="6" stroke-miterlimit="10" />
        </svg>
    </div>
</div>
<div id="theme-switcher-popup" class="sb-theme-popup e-popup-close">
    <ul id="themelist" class="options" role="list">
        @foreach (var theme in Paths.AvailableThemes())
        {
            <li class="@(theme == Paths.DefaultTheme ? "active": "")" id="@theme" role="listitem">
                <span class="sb-theme-select theme-header-icon theme-icon-select"></span>
                <span class="switch-text">@theme</span>
            </li>
        }
    </ul>
</div>
<div class="filterTemplate-wrapper theme-hide">
    <div id="filterDialog"></div>

    <div id="filter-content-template" style="display:none">
        <div class="theme-filter-header">
            <div class="theme-filter-selectall">
                <input type="checkbox" id="filter-selectall" value="Select all">
            </div>

            <div class="form-group" id="filter-form-group">
                <input id="f-search-ip" class="form-control form-control-sm" type="text" placeholder="Search" onkeyup="filterSearch(document.getElementById('f-search-ip').value)">
            </div>

        </div>
        <div class="theme-filter-body">

            <div class="cat-chbx">
                <input type="checkbox" id="f-cat-grids" value="Grids">
            </div>
            <div class="comp-chbx" data="f-cat-grids">
                <input type="checkbox" id="comp-grid" value="Grid">
                <input type="checkbox" id="comp-pivotview" value="PivotTable">
                <input type="checkbox" id="comp-treegrid" value="Treegrid">
