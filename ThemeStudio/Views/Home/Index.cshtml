                <input type="checkbox" id="comp-schedule" value="Schedule">
                <input type="checkbox" id="comp-gantt" value="Gantt">
            </div>
            <div class="theme-div-sep"></div>
            <div class="cat-chbx">
                <input type="checkbox" id="f-cat-editors" value="EDITORS">
